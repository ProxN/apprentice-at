{{#forEach people}}
<li class="person">
  <h3 class="person__name">{{person.name}}</h3>
  <p class="person__summary">{{person.summary}}</p>
  <ul class="person__links">
    <li><a href="{{person.person_url}}">Personal Site</a></li>
    <li><a href="{{person.github_url}}">GitHub</a></li>
    <li><a href="{{person.twitter_url}}">Twitter</a></li>
  </ul>
</li>
{{/forEach}}